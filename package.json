{
  "name": "Micro-Site-Template",
  "version": "0.0.0",
  "description": "An example repository to help users get started with React development on Yext Pages",
  "author": "sumo@yext.com",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "dev": "pages dev",
    "build": "pages build",
    "build:serve": "npm run build && yext pages serve",
    "serve": "yext pages serve",
    "features": "pages generate features",
    "fmt": "prettier -w .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf .artifact-output .yext/ dist/ localData sites-rendered-output",
    "generate-notices": "generate-license-file --input package.json --output ./THIRD-PARTY-NOTICES --overwrite",
    "install": "npm install esbuild",
    "install_m2": "npm i esbuild-darwin-64 -f && npm cache --force clean && npm install --force",
    "prod": "pages prod"
  },
  "engines": {
    "node": "^18.0.0 || >=20.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.12",
    "@heroicons/react": "^2.0.14",
    "@material-tailwind/html": "^2.0.0",
    "@material-tailwind/react": "^2.0.1",
    "@yext/custom-field-debugger": "2.0.9",
    "@yext/pages": "1.0.0",
    "@yext/schema-wrapper": "1.0.2",
    "@yext/search-headless-react": "^2.4.0",
    "@yext/search-ui-react": "^1.5.0",
    "@yext/sites-react-components": "^0.1.16",
    "@yext/types": "0.1.10-alpha",
    "classnames": "^2.3.2",
    "clipboard-copy": "^4.0.1",
    "esbuild": "^0.17.19",
    "markdown-to-jsx": "^7.3.2",
    "pure-react-carousel": "1.30.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^4.0.8",
    "react-icons": "^4.9.0",
    "react-json-view-lite": "^1.2.1",
    "react-markdown": "^8.0.7",
    "react-phone-number-input": "^3.2.19",
    "react-router-dom": "^6.15.0",
    "react-tooltip": "^5.25.1",
    "tailwind-merge": "^1.6.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.49",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@yext/cobalt": "0.0.1",
    "@yext/eslint-config": "1.0.0",
    "@yext/eslint-plugin-export-star": "1.1.0",
    "@yext/pages": "1.0.0",
    "@yext/pages-components": "1.0.0-rc.0",
    "@yext/sites-react-components": "^0.1.16",
    "autoprefixer": "^10.4.8",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.7",
    "generate-license-file": "^1.3.0",
    "postcss": "^8.4.16",
    "postcss-mixins": "^9.0.3",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3",
    "vite": "^5.1.4"
  }
}
